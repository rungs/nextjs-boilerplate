{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env next dev",
    "uat": "env-cmd -f .env.uat next build",
    "prd": "env-cmd -f .env.prd next build",
    "start": "env-cmd -f .env next start",
    "start-dev": "env-cmd -f .env.dev next start",
    "start-uat": "env-cmd -f .env.uat next start",
    "start-prd": "env-cmd -f .env.prd next start",
    "build": "next build",
    "build:dev": "env-cmd -f .env.dev next build",
    "build:uat": "env-cmd -f .env.uat next build",
    "build:prd": "env-cmd -f .env.prd next build",
    "lint": "next lint",
    "test": "jest",
    "test:u": "test -- -u",
    "test:watch": "jest --watch",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage": "jest --coverage --watchAll=false"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.28.0",
    "@azure/msal-common": "^14.16.0",
    "@azure/msal-react": "^2.2.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.3.0",
    "@mui/lab": "^6.0.0-beta.21",
    "@mui/material": "^6.3.1",
    "@mui/styled-engine-sc": "^6.3.1",
    "@mui/x-data-grid": "^7.23.3",
    "@mui/x-date-pickers": "^7.23.3",
    "@reduxjs/toolkit": "^2.5.0",
    "@tabler/icons": "^3.26.0",
    "@tabler/icons-react": "^3.26.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "env-cmd": "^10.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "moment": "^2.30.1",
    "next": "14.2.21",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.54.2",
    "react-perfect-scrollbar": "^1.5.8",
    "react-query": "^3.39.3",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "sass": "^1.83.0",
    "styled-components": "^6.1.14",
    "tailwindcss": "^3.4.17",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.21"
  }
